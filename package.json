  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/jsonld": "^1.5.0",
    "@types/lodash": "^4.14.141",
    "@types/node": "^12.7.11",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-watch": "^6.0.1",
    "jest": "^24.0.0",
    "jest-fetch-mock": "^2.1.2",
